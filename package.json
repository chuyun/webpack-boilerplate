{
  "name": "webpack-boilerplate",
  "version": "3.0.1",
  "description": "Sensible webpack 5 boilerplate using Babel and PostCSS.",
  "main": "index.ts",
  "author": "Tania Rascia",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "prettier --write 'src/**/*.js'",
    "release": "npx standard-version",
    "release:first": "npx run release --first-release",
    "release:alpha": "npx standard-version --prerelease alpha",
    "release:beta": "npx standard-version --prerelease beta",
    "release:rc": "npx standard-version --prerelease rc",
    "release:minor": "npx standard-version -- --release-as minor",
    "release:patch": "npx standard-version -- --release-as patch",
    "commit": "git cz"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.5",
    "sass-loader": "^12.2.0",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:taniarascia/webpack-boilerplate"
  },
  "dependencies": {
    "@rematch/core": "^2.2.0",
    "@rematch/immer": "^2.1.3",
    "@rematch/loading": "^2.1.2",
    "@rematch/select": "^3.1.2",
    "@rematch/updated": "^2.1.2",
    "connected-react-router": "^6.9.2",
    "immer": "^9.0.12",
    "rambda": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
